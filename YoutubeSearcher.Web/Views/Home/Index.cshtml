<div class="row">
    <div class="col-md-6 col-md-offset-3 well">
        <div class="row">
            <div class="col-md-6">
                <div class='input-group date from'>
                    <input id="from" type='text' class="form-control" name="from" placeholder="From" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
            <div class="col-md-6">
                <div class='input-group date to'>
                    <input id="to" type='text' class="form-control" name="to" placeholder="to" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
        </div>
        <br />
        <input type="text" id="seachText" placeholder="Search Youtube" name="search" class="form-control" />
        <br />
        <button class="btn btn-success">Search</button>
    </div>
</div>

<div class="row">
    <div class="col-md-10 col-md-offset-1">
        <table class="table table-striped table-bordered">
            <tr>
                <th>Thumbnail</th>
                <th>Title</th>
                <th>Published Date</th>
            </tr>
        </table>
    </div>
</div>


@section scripts
{
    <script src="~/Scripts/index.js"></script>
}